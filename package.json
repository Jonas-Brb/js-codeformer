		"approvals": "^3.0.5",
		"chai": "^4.3.4",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	},
	"dependencies": {
		"@typescript-eslint/typescript-estree": "^4.28.3",
		"espree": "^8.0.0",
		"estraverse": "^5.2.0"
