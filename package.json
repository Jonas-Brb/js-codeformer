{
	"name": "js-codeformer",
	"displayName": "JS CodeFormer",
	"description": "A refactoring and coding tool for building better source",
	"version": "0.4.1",
	"publisher": "cmstead",
	"repository": {
		"type": "git",
		"url": "https://github.com/cmstead/js-codeformer.git"
	},
	"engines": {
		"vscode": "^1.56.0"
	},
	"categories": [
		"Other"
	],
	"scripts": {
		"lint": "eslint .",
		"test-only": "mocha ./test/**/*.test.js",
		"test": "npm run test-only && gittey commit",
		"build": "rm *.vsix & vsce package",
		"push-patch": "npm run test-only && gittey push-patch",
		"push-minor": "npm run test-only && gittey push-minor"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"@types/vscode": "^1.56.0",
		"approvals": "^3.0.5",
		"chai": "^4.3.4",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vsce": "^1.96.1",
		"vscode-test": "^1.5.2"
	},
	"dependencies": {
		"@typescript-eslint/typescript-estree": "^4.28.3",
		"estraverse": "^5.2.0"
	},
	"main": "./extension.js",
	"activationEvents": [
		"onCommand:cmstead.js-codeformer.extractMethod",
		"onCommand:cmstead.js-codeformer.extractVariable"
	],
	"contributes": {
		"languages": [
			{
				"id": "javascript",
				"extensions": [
					".js",
					".vue"
				],
				"aliases": [
					"JS"
				]
			},
			{
				"id": "vue",
				"extensions": [
					".vue",
					".js"
				],
				"aliases": [
					"Vue"
				]
			},
			{
				"id": "javascriptreact",
				"extensions": [
					".jsx",
					".js"
				],
				"aliases": []
			},
			{
				"id": "typescript",
				"extensions": [
					".ts"
				],
				"aliases": [
					"TS"
				]
			},
			{
				"id": "typescriptreact",
				"extensions": [
					".tsx",
					".ts"
				],
				"aliases": []
			},
			{
				"id": "html",
				"extensions": [
					".htm",
					".html"
				],
				"aliases": [
					"HTML"
				]
			}
		],
		"commands": [
			{
				"command": "cmstead.js-codeformer.extractMethod",
				"title": "JS Codeformer: Extract Method"
			},
			{
				"command": "cmstead.js-codeformer.extractVariable",
				"title": "JS Codeformer: Extract Variable"
			}
		]
	}
}
